{
    "java.configuration.updateBuildConfiguration": "interactive",
    "cucumberautocomplete.steps": [
        "cucumber-project/src/test/java/llc/eventa/stepDefination/*java"
    ],
    "cucumberautocomplete.syncfeatures": "cucumber-project/src/test/java/llc/eventa/resources/*feature",
    "cucumberautocomplete.strictGherkinCompletion": true,
    "cucumber.features": [
        "src/test/**/*.feature",
        "features/**/*.feature",
        "tests/**/*.feature",
        "*specs*/**/.feature",
        "src/test/java/llc/eventa/resources**/*.feature"
    ],
    "cucumber.glue": [
        "src/test/llc/eventa/**/*.java",
        "features/**/*.js",
        "features/**/*.java",
        "features/**/*.jsx",
        "features/**/*.ts",
        "features/**/*.tsx",
        "features/**/*.php",
        "features/**/*.py",
        "tests/**/*.py",
        "tests/**/*.rs",
        "features/**/*.rs",
        "features/**/*.rb",
        "*specs*/**/.cs"
    ],
    "java.compile.nullAnalysis.mode": "automatic"
}